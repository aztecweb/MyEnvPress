{
	"name" : "edpittol/myenvpress",
	"description" : "My WordPress development environment.",
	"repositories" : [{
			"type" : "composer",
			"url" : "https://wpackagist.org"
		}, {
			"type" : "package",
			"package" : {
				"name" : "wordpress/wordpress",
				"type" : "webroot",
				"version" : "4.5.2",
				"dist" : {
					"type" : "zip",
					"url" : "https://wordpress.org/wordpress-4.5.2-no-content.zip"
				},
				"require" : {
					"fancyguy/webroot-installer" : "^1.0"
				}
			}
		}
	],
	"require" : {
		"wordpress/wordpress" : "*",
		"wp-cli/wp-cli" : "~0.23"
	},
	"extra" : {
		"webroot-dir" : "public",
		"webroot-package" : "wordpress/wordpress",
		"installer-paths" : {
			"public/wp-content/plugins/{$name}/" : [
				"type:wordpress-plugin"
			],
			"public/wp-content/themes/{$name}/" : [
				"type:wordpress-theme"
			]
		}
	}
}